<h3>Sign In To Your Account Below</h3>
    <hr><br>
      
<h1>Login Here</h1>

<%= form_with url: login_path do |f| %>
    <div>
    <%= f.label :username %>
    <%= f.text_field :username %>
    </div>
</br>
    <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
    </div>
</br>
    <div>
    <%= f.submit 'Login' %>
    </div>
    
<% end %>

<h3>Or Login with Google:</h3>

<%= button_to 'Google', '/auth/google_oauth2' %>

